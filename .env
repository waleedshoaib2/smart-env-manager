# Application
NODE_ENV=development
PORT=4000
LOG_LEVEL=debug

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/mydb
DATABASE_MAX_CONNECTIONS=50
DATABASE_SSL=true

# Redis Cache
REDIS_HOST=localhost
REDIS_PORT=6379
CACHE_TTL=3600

# Security
API_KEYS=32charslongapikeyexample12345678,anotherapikeyexample987654321234567
SECRET_KEY=your-very-long-and-very-secret-key-here
ALLOWED_ORIGINS=http://localhost:3000,https://app.example.com

# Features
ENABLED_FEATURES=audit-logs,multi-factor-auth,api-rate-limiting